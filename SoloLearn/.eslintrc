{
	"parser":
		"babel-eslint",
	"env": {
		"node": true,
		"browser": true,
		"es6": true
	},
	"extends":
		"eslint-config-airbnb",
	"rules": {
		"indent": [
			2,
			"tab"
		],
		"no-tabs": 0,
		"no-console": 0,
		"array-bracket-spacing": [
			2,
			"always"
		],
		"object-curly-spacing": [
			2,
			"always"
		],
		"no-multiple-empty-lines": [
			2,
			{
				"max": 1,
				"maxEOF": 0,
				"maxBOF": 0
			}
		],
		"quotes": [
			2,
			"single"
		],
		"no-use-before-define": [
			2,
			{
				"functions": false
			}
		],
		"prefer-const": 1,
		"complexity": 0,
		"react/jsx-filename-extension": 0,
		"react/prop-types": 0,
		"react/jsx-indent-props": [
			2,
			"tab"
		],
		"react/jsx-indent": [
			2,
			"tab"
		],
		"jsx-a11y/click-events-have-key-events": 0,
		"jsx-a11y/anchor-is-valid": 0,
		"no-underscore-dangle": 0,
		"react/sort-comp": 0,
		"no-nested-ternary": 0
	},
	"settings": {
		"import/resolver": {
				"webpack": {
						"config": "config/webpack.config.dev.js"
				}
		}
	}
}
